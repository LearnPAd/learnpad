bpmn:Activity
  rdf:type owl:Class ;
  rdfs:comment "Former 'Atomar', naming and subclass-structure in accordance to BPMN 2.0 (OMG 2009)"^^xsd:string ;
  rdfs:label "Activity"^^xsd:string ;
