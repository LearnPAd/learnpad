bpmn:Association
  rdf:type owl:Class ;
  rdfs:comment "„An Association is used to link information and Artifacts with BPMN graphical elements.  An arrowhead on the Association indicates a direction of flow (e.g., data), when appropriate.“ (Axway et al., 2009: p.21)\" (Brun 2010)"^^xsd:string ;
  rdfs:label "Association"^^xsd:string ;
  rdfs:subClassOf bpmn:Connector ;
.
